alias vim='nvim'
alias docker-compose='docker compose'
alias composer='~/Workspace/dotfiles/.bin/composer'
alias chelezo='~/Workspace/dotfiles/.bin/chelezo'
alias php='docker run --rm -v $(pwd):/app -w /app shineneko/php php'
alias phpstan='docker run --rm -v $(pwd):/app -w /app phpstan/phpstan'
alias sf='docker-compose run --rm php bin/console'
alias go='docker run --rm -v $(pwd):/app -w /app golang go'
alias proxy='cd ~/Workspace/alpacode/infra/proxy && docker-compose up -d'
alias deno='docker run -it -v $(pwd):/app -w /app denoland/deno deno'
alias pgcli='docker run -it --rm dencold/pgcli'
alias fabric='docker run -e USER=root --rm -v $(pwd):/app -v $HOME/.ssh:/root/.ssh -w /app fabric fab'
alias ansible='~/Workspace/dotfiles/.bin/ansible'
alias ring='~/Workspace/kemeter/ring/target/release/ring'
alias kemeter='~/Workspace/kemeter/kemeter/cli/target/release/kemeter'
alias cat='/usr/bin/batcat'
alias chat='\cat'
alias dive="docker run -ti --rm  -v /var/run/docker.sock:/var/run/docker.sock wagoodman/dive"
alias clip="wl-copy <"
alias cb="wl-copy <"
