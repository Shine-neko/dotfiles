alias vim='nvim'
alias docker-compose='docker compose'
alias composer='~/Workspace/dotfiles/.bin/composer'
alias php='docker run --rm -v $(pwd):/app -w /app shineneko/php php'
alias phpstan='docker run --rm -v $(pwd):/app -w /app phpstan/phpstan'
alias sf='docker-compose run --rm php bin/console'
alias go='docker run --rm -v $(pwd):/app -w /app golang go'
alias proxy='cd ~/Workspace/craft/infra/proxy && docker-compose up -d'
alias deno='docker run -it -v $(pwd):/app -w /app barrage/deno deno'
alias pgcli='docker run -it --rm dencold/pgcli'
alias fabric='docker run -e USER=root --rm -v $(pwd):/app -w /app fabric fab'
alias ansible='~/Workspace/dotfiles/.bin/ansible'
alias ring='~/Workspace/kemeter/ring/target/release/ring'
alias cat='/usr/bin/batcat'
alias chat='\cat'
